<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Carina Demo Tests - Mobile UI (Android)" verbose="1" parallel="methods">

    <test name="Mobile test Login Page on Android ">
        <parameter name="isPerformLogin" value="false" />
        <classes>
            <class name="com.qaprosoft.carina.igor.LoginPageTest">
                <methods>
                    <exclude name="validCredentialsTest"/>
                    <exclude name="validationUiElementsOnLoginPageTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
